<ClickableTextFieldRoundPassword>:
    size_hint_y: None
    height: text_field.height

    MDTextField:
        id: text_field
        hint_text: root.hint_text
        password: True
        icon_left: "key-variant"
        # required: True
        mode: "line"

    MDIconButton:
        icon: "eye-off"
        pos_hint: {"center_y": .5}
        pos: text_field.width - self.width + dp(8), 0
        theme_text_color: "Hint"
        on_release:
            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
            text_field.password = False if text_field.password is True else True
My_Layout:
    screen_Mngr: screen_mngr
    ScreenManager:
        id: screen_mngr
        home: home
        Screen1:
            id: home
            name: "home"
            title: "Sign In"

            MDCard:
                id: md_login
                orientation: "vertical"
                padding: 0, "20dp", 0 , "20dp"
                size_hint: .8, .8 - 0.03
                pos_hint: {"center_x": .5, "center_y": .5}
                shadow_radius: 6
                shadow_offset: 0, 2
                radius: 20
                spacing: "5dp"
                line_color: (0.2, 0.2, 0.2, 0.8)
                elevation: 4

                MDIconButton:
                    icon: "theme-light-dark"
                    pos_hint: {"center_x": .5, "center_y": .5}
                    icon_size: "30sp"
                    icon_color: "red"

                MDLabel:
                    id: sign
                    text: "SIGN IN"
                    halign: "center"
                    valign: "center"
                    pos_hint: {"center_x": .5, "center_y": .5}
                    bold: True
                    font_style: "H5"

                MDTextField:
                    id: login_email
                    hint_text: "Email"
                    icon_left: "email"
                    helper_text: "user@gmail.com"
                    #validator: "email"
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    pos_hint: {"center_x": .5, "y": .5}

                ClickableTextFieldRoundPassword:
                    id: login_pass
                    text: ""
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    hint_text: "Password"
                    pos_hint: {"center_x": .5, "center_y": .5}

                MDRoundFlatButton:
                    id: sign_btn
                    text: "Sign In"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": .5, "center_y": .5}
                    on_press: app.login()
                             
                MDFlatButton:
                    text: "Forget Password ?"
                    theme_text_color: "Custom"
                    text_color: "orange"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_release: screen_mngr.current = "fgt_pass"

                MDFlatButton:
