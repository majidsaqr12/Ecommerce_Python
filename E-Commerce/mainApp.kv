My_Layout:
    screen_Mngr: screen_mngr
    MDScreenManager:
        id: screen_mngr
        home: home
                
        Screen1:
            id: home
            name: "home"
            title: "Sign In"

            MDCard:
                id: md_login
                orientation: "vertical"
                padding: 0, "20dp", 0 , "20dp"
                size_hint: .8, .8 - 0.03
                pos_hint: {"center_x": .5, "center_y": .5}
                shadow_radius: 6
                shadow_offset: 0, 2
                spacing: "5dp"
                radius: 25
                line_color: (0.0, 0.0, 0.0, 0.0)
                md_bg_color: "DDDDDD"
                unfocus_color: "red"
                focus_color: "red"
                elevation: 4

                MDIconButton:
                    icon: "theme-light-dark"
                    pos_hint: {"center_x": .5, "center_y": .5}
                    icon_size: "30sp"
                    icon_color: "red"

                MDLabel:
                    id: sign
                    text: "SIGN IN"
                    halign: "center"
                    valign: "center"
                    pos_hint: {"center_x": .5, "center_y": .5}
                    #bold: True
                    font_style: "H5"
                    #font_name: "comic"
                    italic: True

                MDTextField:
                    id: login_email
                    hint_text: "Email"
                    icon_left: "email"
                    helper_text: "user@gmail.com"
                    #validator: "email"
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    pos_hint: {"center_x": .5, "y": .5}

                MDTextField:
                    id: login_pass
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    hint_text: "Password"
                    icon_left: "key-variant"
                    password: True
                    pos_hint: {"center_x": .5, "center_y": .5}

                MDRoundFlatButtonHover:
                    id: sign_btn
                    text: "Sign In"
                    md_bg_color: "DDDDDD"
                    text_color: "red"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": .5, "center_y": .5}
                    on_press:
                        app.login()
                             
                MDFlatButtonHover:
                    text: "Forget Password ?"
                    theme_text_color: "Custom"
                    text_color: "orange"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_press: screen_mngr.current = "fgt_pass"

                MDFlatButtonHover:
                    text: "Join With US"
                    theme_text_color: "Custom"
                    text_color: "orange"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": .5, "center_y": .2}
                    on_press: screen_mngr.current = "signup"

        Signup_Screen:
            id: signup
            name: "signup"
            MDCard:
                padding: 0, "10dp", 0 , "1dp"
                orientation: "vertical"
                spacing: "1dp"
                size_hint: .8 - 0.01 + 0.05, .9 + 0.02
                md_bg_color: "ffffff"
                pos_hint: {"center_x": .5, "center_y": .5 + 0.01}
                radius: 25
                line_color: (0.0, 0.0, 0.0, 0.0)
                md_bg_color: "DDDDDD"
                elevation: 4

                MDLabel:
                    text: "SIGN UP"
                    halign: "center"
                    valign: "center"
                    pos_hint: {"center_x": 0.5, "y": .8}
                    # bold: True
                    italic: True
                    font_style: "H5"

                MDTextField:
                    id: full_name
                    hint_text: "Full name"
                    icon_left: "account"
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    pos_hint: {"x": .1 - 0.02, "y": .6}
                    text_color_normal: "gray"

                MDTextField:
                    id: email_up
                    hint_text: "Email"
                    icon_left: "email"
                    helper_text: "user@gmail.com"
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    pos_hint: {"x": .1 - 0.02, "y": .9}
                    text_color_normal: "gray"
                
                MDTextField:
                    id: phone
                    hint_text: "phone"
                    icon_left: "phone"
                    text: "+20"
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    pos_hint: {"x": .1 - 0.02, "y": .8}
                    text_color_normal: "gray"
                    max_text_length: 13
                
                # MDBoxLayout:
                #     orientation: "horizontal"
                #     spacing: "10dp"
                #     pos_hint: {"x": .1 - 0.02, "y": .8}
                #     size_hint_x: 0.9 - 0.05

                #     MDTextField:
                #         id: confirm_state
                #         hint_text: "State"
                #         mode: "line"
                #         size_hint_x: 0.5

                #     MDTextField:
                #         id: confirm_city
                #         hint_text: "City"
                #         mode: "line"
                #         size_hint_x: 0.5
                        
                #     MDTextField:
                #         id: confirm_street
                #         hint_text: "Street"
                #         mode: "line"
                #         size_hint_x: 0.5

                MDTextField:
                    id: sign_up_pass
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    hint_text: "Password"
                    icon_left: "key-variant"
                    password: True
                    pos_hint: {"x": .1 - 0.02, "y": .6}

                MDTextField:
                    id: sign_up_confirm_pass
                    size_hint_x: None
                    size_hint: 0.8,0.8
                    hint_text: "Confirm Password"
                    icon_left: "key-variant"
                    password: True
                    pos_hint: {"x": .1 - 0.02, "y": .5}

                MDRoundFlatButtonHover:
                    id: sign_up_button
                    text: "Sign up"
                    md_bg_color: "DDDDDD"
                    text_color: "red"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": 0.5, "y": .4}
                    on_press: app.sign_up()

                
                MDLabel:
                    text: "Have an account ?"
                    halign: "center"
                    valign: "center"
                    size_hint: 0.8,0.6
                    pos_hint: {"center_x": 0.5, "y": .3}
                    italic: True

                MDFlatButtonHover:
                    text: "Sign in"
                    theme_text_color: "Custom"
                    text_color: "orange"
                    size_hint: 0.5,0.2
                    pos_hint: {"center_x": 0.5, "y": 0.3}
